{
  "kind": "collectionType",
  "collectionName": "shipping_origins",
  "info": {
    "singularName": "shipping-origin",
    "pluralName": "shipping-origins",
    "displayName": "ShippingOrigin",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "freight_rates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::freight-rate.freight-rate",
      "mappedBy": "shipping_origin"
    },
    "shipments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shipment.shipment",
      "mappedBy": "shipping_origin"
    }
  }
}
