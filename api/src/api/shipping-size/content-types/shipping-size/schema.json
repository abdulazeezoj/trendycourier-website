{
  "kind": "collectionType",
  "collectionName": "shipping_sizes",
  "info": {
    "singularName": "shipping-size",
    "pluralName": "shipping-sizes",
    "displayName": "ShippingSize"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "unit": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "comment": {
      "type": "text"
    },
    "freight_rates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::freight-rate.freight-rate",
      "mappedBy": "shipping_size"
    },
    "shipments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shipment.shipment",
      "mappedBy": "shipping_size"
    }
  }
}
